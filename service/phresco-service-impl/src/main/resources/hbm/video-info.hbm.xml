<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
 <class name="com.photon.phresco.model.VideoInfo" table="video_info">
  <id name="id" type="string">
   <meta attribute="use-in-equals">true</meta>
   <column name="id" length="255"/>
  </id>
  <property generated="never" lazy="false" name="name" type="string">
   <column length="255" name="name"/>
  </property>
  <property generated="never" lazy="false" name="description" type="string">
   <column length="255" name="description"/>
  </property>
  <property generated="never" lazy="false" name="imageurl" type="string">
   <column length="255" name="imageurl"/>
  </property>
  <property generated="never" lazy="false" name="categories" type="string">
   <column length="255" name="categories"/>
  </property>
  <property generated="never" lazy="false" name="helpText" type="string">
   <column length="255" name="helpText"/>
  </property>
 </class>
 <query name="service.getVideoInfo">from VideoInfo</query>
</hibernate-mapping>
